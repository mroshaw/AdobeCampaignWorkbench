<%

function perso(strPerso) {
 var strStart = '<' + '%';
 var strEnd = '%' + '>';
 return strStart + strPerso + strEnd;
}
var begin = '&lt;' + '%';
var end = '%' + '&gt;';

function bloc(strPerso)
{
  var strStart = '<' + '%' + '@ include view="';
  var strEnd = '" %' + '>';
  return strStart + strPerso + strEnd;
}
%>
<!-- M72_MCPaymentScheduleDetails -->
<%= bloc('ssgMessageCenterCommonFunctions') %>
<%= perso('var eventType=message.delivery.triggerMessage.eventType;') %>
<tr>
  <td align="center" valign="top">
    <div id="subscription-payment-schedule-details" class="table">
      <table class="wFull carbon-bg" cellpadding="0" cellspacing="0" border="0" role="presentation" style="background-color:#FEFEFE; width: 600px;">
        <tr>
          <td class="white-txt" align="left" valign="top">
            <h4 class="tMar-16" style="margin: 24px 32px 16px;"><%=ctx.@HEADER%></h4>
          </td>
        </tr>
        <tr>
          <td class="xPad-16 bPad-16" align="center" valign="top" style="padding: 0px 32px 24px;">
            <table class="wFull smoke-bg" cellpadding="0" cellspacing="0" border="0" role="presentation" style="width: 536px; background-color: #F5F5F5; border-radius: 8px;">
              <div name="payments-received">
                <tr>
                  <td class="white-txt" align="left" valign="top">
                    <h6 style="margin: 16px 16px 0;"><%=ctx.@PAYMENTTEXT%></h6>
                  </td>
                </tr>
                <tr>
                  <td align="center" valign="top" style="padding: 8px 16px 16px;">
                    <table class="wFull white-bg" cellpadding="0" cellspacing="0" border="0" role="presentation" style="width: 504px; background-color: #FEFEFE; border-radius: 8px;">
                      <div name="horisontal-spacer-4px">
                        <tr>
                          <td colspan="2" align="center" valign="top" style="font-size: 4px; mso-line-height-rule: exactly; line-height: 4px; height: 4px;">&nbsp;</td>
                        </tr>
                      </div>
                      <%=perso("var index=0; var nextPayment= new Date(0);")%>
                      <%=perso("for each (instalment in rtEvent.ctx.order.subscription.instalments.instalment) {")%>
                      <%=perso("var success=instalment.success; var displayPrice=instalment.amount.displayPrice; var dueDateStr=formatDateMC(instalment.dueDate, eventType); var dueDate=parseTimeStamp(instalment.dueDate); var paidDateStr=formatDateMC(instalment.paidDate, eventType);") %>
                      <%=perso("var status=success=='true' ? 'PAID' : 'UNPAID';")
                      <%=perso('if(status=="PAID"){')%>
                      <!-- *COMPONENT START* Payment line paid -->
                      <div name="payment-line-paid">
                        <tr>
                          <td align="left" valign="middle" style="padding: 4px 0;">
                            <h6 class="xMar-12" style="margin: 0 12px; color: #016C42;"><%=person('=paidDateStr')%></h6>
                          </td>
                          <td align="right" valign="middle" style="padding: 4px 0;">
                            <h6 class="xMar-12" style="margin: 0 12px; color: #016C42;"><span style="font-size: 14px; line-height: 20px; font-weight: normal;"><%=perso('=status')%></span> <%=perso('=displayPrice')%></h6>
                          </td>
                        </tr>
                      </div>
                      <!-- Payment line paid *END* -->
                      <!-- *COMPONENT START* Payment line unpaid -->
                      <%=perso("}else{")%>
                      <div name="payment-line-unpaid">
                        <tr>
                          <td align="left" valign="middle" style="padding: 4px 0;">
                            <h6 class="xMar-12" style="margin: 0 12px; color: #016C42;"><%=person('=paidDateStr')%></h6>
                          </td>
                          <td align="right" valign="middle" style="padding: 4px 0;">
                            <h6 class="xMar-12" style="margin: 0 12px; color: #016C42;"><span style="font-size: 14px; line-height: 20px; font-weight: normal;"><%=perso('=status')%></span> <%=perso('displayPrice')%></h6>
                          </td>
                        </tr>
                      </div>
                      <!-- Payment line unpaid *END* -->
                      <%=perso("}")%>
                      <%=perso("index++;")%>
                      <%=perso("}")%>
                      <div name="horisontal-spacer-4px">
                        <tr>
                          <td colspan="2" align="center" valign="top" style="font-size: 4px; mso-line-height-rule: exactly; line-height: 4px; height: 4px;">&nbsp;</td>
                        </tr>
                      </div>
                    </table>
                  </td>
                </tr>
              </div>
              <!-- *COMPONENT START* Next payments due -->
              <div name="next-payment-due">
                <tr>
                  <td class="white-txt" align="left" valign="top">
                    <p class="font-14" style="margin: 0 16px;"><%=ctx.@NEXTPAYMENTSDUE%></p>
                  </td>
                </tr>
                <tr>
                  <td align="center" valign="top" style="padding: 8px 16px 0;">
                    <table class="wFull white-bg" cellpadding="0" cellspacing="0" border="0" role="presentation" style="width: 504px; background-color: #FEFEFE; border-radius: 8px;">
                      <div name="horisontal-spacer-4px">
                        <tr>
                          <td colspan="2" align="center" valign="top" style="font-size: 4px; mso-line-height-rule: exactly; line-height: 4px; height: 4px;">&nbsp;</td>
                        </tr>
                      </div>
                      <!-- *SUB-COMPONENT START* Payment line pay -->
                      <div name="payment-line-pay">
                        <tr>
                          <td align="left" valign="middle" style="padding: 4px 0;">
                            <p class="xMar-12" style="margin: 0 12px;">TBC</p>
                          </td>
                          <td align="right" valign="middle" style="padding: 4px 0;">
                            <p class="xMar-12" style="margin: 0 12px;"><span style="font-size: 14px; line-height: 20px;">TBC</span> TBC</p>
                          </td>
                        </tr>
                      </div>
                      <!-- Payment line pay *END* -->
                      <div name="horisontal-spacer-4px">
                        <tr>
                          <td colspan="2" align="center" valign="top" style="font-size: 4px; mso-line-height-rule: exactly; line-height: 4px; height: 4px;">&nbsp;</td>
                        </tr>
                      </div>
                    </table>
                  </td>
                </tr>
                <tr>
                  <td align="center" valign="top" style="font-size: 16px; mso-line-height-rule: exactly; line-height: 16px; height: 16px;">
                    &nbsp;
                  </td>
                </tr>
              </div>
              <!-- Next payments due *END* -->
            </table>
          </td>
        </tr>
      </table>
    </div>
  </td>
</tr>

<!-- EMAIL TABLE MODULES END -->